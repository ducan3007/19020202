<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="193.0" prefWidth="443.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.addwordController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Nhập từ muốn thêm:">
            <font>
                <Font size="21.0" />
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="90.0" prefHeight="45.0" prefWidth="370.0" text="Nhập nghĩa của từ :">
            <font>
                <Font size="21.0" />
            </font>
        </Label>
        <TextField fx:id="Word" layoutX="14.0" layoutY="45.0" onAction="#getWord" prefHeight="45.0" prefWidth="363.0">
            <font>
                <Font size="21.0" />
            </font>
        </TextField>
        <TextField fx:id="Explain" layoutX="14.0" layoutY="133.0" onAction="#getDifinition" prefHeight="45.0" prefWidth="363.0">
            <font>
                <Font size="21.0" />
            </font>
        </TextField>
    </children>
</AnchorPane>
